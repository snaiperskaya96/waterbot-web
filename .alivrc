{
    "quiet": true,
    "port": 3000,
    "http2": false,
    "pathIndex": "client/dev",
    "only": [
        "client/dev/**/*"
    ],
    "static": [
        "node_modules",
        "node_modules/fullcalendar/dist",
        "node_modules/jquery/dist",
        "node_modules/moment",
        "jspm_packages"
    ],
    "proxy": true,
    "proxyWhen": "/api/*",
    
    "proxyTarget": "http://localhost:3333"
    
}
